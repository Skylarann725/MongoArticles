    <!-- Nav Bar Setup -->
<div class="navbar-fixed">
    <nav>
        <div class="nav-wrapper #212121 grey darken-4">
            <a href="#" class="brand-logo center">Live Science Article Finder</a>
            <ul id="nav-mobile" class="left hide-on-med-and-down">
                <li id="index_hbds"><a href="/">Home</a></li>
                <li id="saved_hbds"><a href="/saved">Saved Articles</a></li>
                <li><a class="waves-effect waves-light btn" id="scraped">Get New Articles!</a></li>
            </ul>
        </div>
    </nav>
</div>
    <!-- Modal Structure -->
    <div id="resultsModal" class="modal">
        <div class="modal-content center-align">
            <h4>Added New Articles!</h4>
        </div>
        <div class="modal-footer">
            <a href="/" class="modal-action modal-close waves-effect waves-green btn-flat">Ok</a>
        </div>
    </div>
    <!-- Paralax -->
        <div id="index-banner" class="parallax-container">
            <div class="section no-pad bot"
                <div class="container">
                    <br>
                    <br>
                    <h1>Live Science Articles!</h1>
                        <div class="row center">
                            <h4>Click the "Get New Articles" button to get all of the latest Live Science Articles!</h4>
                        </div>
                </div>
            </div>
        </div>
        <div class="parallax"><img src="./assets/img/science3.jpg"></div>
    <!-- Article Boxes -->  
    <div class="article-container">
        <ul class="collection with-header">
        {{#each articles}}            
            <div class="title" id="article_title">
                <li class="collection-header #e1f5fe light-blue lighten-5">       
                    <div class="title_info"><a href="http://www.livescience.com/{{this.link}}"><img src="{{this.image}}"></a><h5>{{this.title}}
                    {{#if this.saved}}
                        <a class="waves-effect waves-light btn #ff6e40 deep-orange accent-2 save_article" data-id="{{this._id}}">Saved</a></h5></div>
                    {{/if}}
                    {{#unless this.saved}}
                        <a class="waves-effect waves-light btn save_article" data-id="{{this._id}}">Save Article</a></h5></div>
                    {{/unless}}
                </li>
            </div>
            <div class="summary" id="article_summary">
                <li class="collection-item">{{this.content}}<a href="http://www.livescience.com/{{this.link}}">Here</a></li>
            </div> 
        {{/each}}
        </ul>          
    </div>
     
